{
  "name": "Laravel.JpRecipe",
  "version": "0.0.1",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "materialize": "*",
    "react": "0.*",
    "highlightjs" : "*",
    "font-awesome": "*"
  },
  "exportsOverride": {
  },
  "overrides": {
    "jquery": {
      "main": "**/*.min.js"
    },
    "font-awesome": {
      "main": [
        "**/*.min.css",
        "fonts/**/*"
      ]
    },
    "materialize": {
      "main": [
        "**/*.min.js",
        "**/*.min.css",
        "font/**/*"
      ]
    },
    "react": {
      "main": [
        "**/*.min.js"
      ]
    },
    "highlightjs": {
      "main": [
        "**/*.pack.js",
        "**/monokai_sublime.css",
        "**/monokai.css"
      ]
    }
  }
}
