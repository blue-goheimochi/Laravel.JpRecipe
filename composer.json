{
  "name": "ytake/laravel-jp-recipe",
  "description": "The Laravel Framework. jp recipe site",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "yuuki takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "ext-json": "*",
    "laravel/framework": "5.1.*",
    "predis/predis": "1.*",
    "guzzlehttp/guzzle": "6.*",
    "nocarrier/hal": "0.9.*",
    "zendframework/zend-feed": "2.*",
    "zendframework/zend-servicemanager": "2.*",
    "zendframework/zend-http": "2.*",
    "erusev/parsedown-extra": "*",
    "laravelcollective/annotations": "5.1.*",
    "ytake/laravel-aspect": "~0.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "4.*",
    "phpspec/phpspec": "~2.1",
    "mockery/mockery": "*",
    "doctrine/dbal": "~2.3",
    "barryvdh/laravel-ide-helper": "2.*",
    "satooshi/php-coveralls": "dev-master",
    "phpmd/phpmd" : "*",
    "laravel/homestead": "*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/common.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "suggest": {
    "halleck45/phpmetrics": "provides various metrics about PHP projects."
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php artisan optimize"
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
